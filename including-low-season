<!-- WeTravel embed (load once) -->
<script src="https://cdn.wetravel.com/widgets/embed_checkout.js"></script>

<script>
  // Hover behavior (yours)
  const dropdownTriggerButton = document.getElementById('wetravel-bnb-dropdown');
  const dropdownButtonListContainer = document.getElementById('wetravel-bnb-list');

  dropdownButtonListContainer.addEventListener('mouseover', () => {
    dropdownTriggerButton.style.borderRadius = '32px 32px 0px 0px';
    dropdownButtonListContainer.style.display = 'block';
  });

  dropdownButtonListContainer.addEventListener('mouseleave', () => {
    dropdownTriggerButton.style.borderRadius = '32px';
    dropdownButtonListContainer.style.display = 'none';
  });

  dropdownTriggerButton.addEventListener('mouseover', () => {
    dropdownTriggerButton.style.borderRadius = '32px 32px 0px 0px';
    dropdownButtonListContainer.style.display = 'block';
  });

  dropdownTriggerButton.addEventListener('mouseleave', () => {
    dropdownTriggerButton.style.borderRadius = '32px';
    dropdownButtonListContainer.style.display = 'none';
  });

  // Optional: click-to-toggle (better on mobile). Keep or remove.
  dropdownTriggerButton.addEventListener('click', (e) => {
    e.preventDefault();
    const open = dropdownButtonListContainer.style.display === 'block';
    dropdownButtonListContainer.style.display = open ? 'none' : 'block';
    dropdownTriggerButton.style.borderRadius = open ? '32px' : '32px 32px 0 0';
  });

  // Optional: close when clicking outside (nice UX)
  document.addEventListener('click', (e) => {
    const wrap = dropdownButtonListContainer.parentElement;
    if (!wrap.contains(e.target) && dropdownButtonListContainer.style.display === 'block') {
      dropdownButtonListContainer.style.display = 'none';
      dropdownTriggerButton.style.borderRadius = '32px';
    }
  });
</script>
