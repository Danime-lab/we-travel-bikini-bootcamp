        <script src="https://cdn.wetravel.com/widgets/embed_checkout.js"></script>
        <script>
            const dropdownTriggerButton = document.getElementById('wetravel-bnb-dropdown');
            const dropdownButtonListContainer = document.getElementById('wetravel-bnb-list');

            dropdownButtonListContainer.addEventListener('mouseover', (ev) => {
                dropdownTriggerButton.style.borderRadius = '32px 32px 0px 0px';
                dropdownButtonListContainer.style.display = 'block';
            });

            dropdownButtonListContainer.addEventListener('mouseleave', (ev) => {
                dropdownTriggerButton.style.borderRadius = '32px';
                dropdownButtonListContainer.style.display = 'none';
            });

            dropdownTriggerButton.addEventListener('mouseover', (ev) => {
                dropdownTriggerButton.style.borderRadius = '32px 32px 0px 0px';
                dropdownButtonListContainer.style.display = 'block';
            });

            dropdownTriggerButton.addEventListener('mouseleave', (ev) => {
                dropdownTriggerButton.style.borderRadius = '32px';
                dropdownButtonListContainer.style.display = 'none';
            });
        </script><style>
html.squarespace-damask .sqs-blockStatus {
display: none !important;
}
</style>
